<h1>KVDB</h1>
<h2><%= @video.title %></h2>

<iframe width="420" height="315" src=<%= @video.url %>></iframe>

<p>Actors: </p>
<ul>
  <% @actors.each do |actor|  %>
    <li><%= actor.name %></li>
  <% end %>
</ul>

<p>Categories:</p>
<ul>
  <% @categories.each do |category|  %>
    <li><%= category.name %></li>
  <% end %>
</ul>
